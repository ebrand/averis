      "vhost": "commerce",
      "destination": "product.events",
      "destination_type": "queue",
      "routing_key": "product.deleted",
      "arguments": {}
    },
    {
      "source": "product.exchange",
      "vhost": "commerce",
      "destination": "product.maintenance",
      "destination_type": "queue",
      "routing_key": "product.maintenance.*",
      "arguments": {}
    },
    {
      "source": "product.exchange",
      "vhost": "commerce",
      "destination": "product.notifications",
      "destination_type": "queue",
      "routing_key": "product.notification.*",
      "arguments": {}
