  "queues": [
    {
      "name": "product.events",
      "vhost": "commerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000
      }
    },
    {
      "name": "product.maintenance",
      "vhost": "commerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 5000
      }
    },
    {
      "name": "product.notifications",
      "vhost": "commerce",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 1000
      }
    }
  ],
  "exchanges": [
    {
      "name": "product.exchange",
      "vhost": "commerce",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
